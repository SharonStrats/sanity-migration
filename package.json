{
  "name": "migration",
  "version": "1.0.0",
  "private": true,
  "description": "Nura Sanity Migration",
  "author": "Sharon Stratsianis",
  "keywords": [
    "migration"
  ],
  "type": "module",
  "main": "./build/index.js",
  "scripts": {
    "build": "yarn type-check && yarn lint && yarn test",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "loaddocuments": "node dist/index.js",
    "loadimages": "node dist/images.js",
    "deletedocuments": "node dist/delete.js",
    "lint": "yarn --cwd ../../ lint",
    "test": "yarn --cwd ../../ test",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@sanity/client": "^2.10.5",
    "@sanity/types": "^2.10.5",
    "@types/dotenv": "^8.2.0",
    "@types/node": "^15.6.1",
    "@types/readable-stream": "^2.3.10",
    "dotenv": "^10.0.0",
    "lodash": "^4.17.21",
    "memory-streams": "^0.1.3",
    "typescript": "^4.3.2"
  },
  "devDependencies": {
    "ts-node-dev": "^1.1.6"
  }
}
